define service{
  service_description    HTTP UNIQUE_NAME
  use            yolo-linux-service
  register       0
  host_name      yolo-linux-server
  check_command  yolo_check_http!BASE.DOMAIN
}

define service{
  service_description    Backup UNIQUE_NAME
  use            yolo-linux-service
  register       0
  host_name      yolo-linux-server
  check_interval 60
  retry_interval 15
  check_period period_backup
  check_command  yolo_check_backup!APPLICATION!SERVER!SERVICE!DOMAIN!SAAS
}

